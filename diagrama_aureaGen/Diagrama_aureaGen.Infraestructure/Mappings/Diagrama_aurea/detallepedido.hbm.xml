<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="Diagrama_aureaGen.Infraestructure"
		                   namespace="Diagrama_aureaGen.Infraestructure.EN.Diagrama_aurea">
		    
		    
			
				<class name="DetallePedidoNH" table="DetallePedido"> 
						<id name="IdDetalle" column="idDetalle"/> 
			
					
					<property name="Cantidad" type="integer"> 
						<column name="cantidad"  not-null="true"/>
					</property>
					<property name="PrecioUnitario" type="float"> 
						<column name="precioUnitario"  not-null="true"/>
					</property>
					<property name="Subtotal" type="float"> 
						<column name="subtotal"  not-null="true"/>
					</property>
			
			
			
		  			
						<many-to-one name="Incluido_en" class="Diagrama_aureaGen.Infraestructure.EN.Diagrama_aurea.ProductoNH" >	
								<column name="FK_idProductoProducto_idProducto"/> 
						</many-to-one>
											 					
			
		  			
						<many-to-one name="Pedido" class="Diagrama_aureaGen.Infraestructure.EN.Diagrama_aurea.PedidoNH" cascade="delete">	
								<column name="FK_idPedidoPedido_idPedido"/> 
						</many-to-one>
											 					
			
			
			
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="DetallePedidoNHReadAllHQL">from DetallePedidoNH</query>
		</hibernate-mapping>
	